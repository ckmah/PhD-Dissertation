@article{lathiaCancerStemCells2015,
  title = {Cancer Stem Cells in Glioblastoma},
  author = {Lathia, Justin D. and Mack, Stephen C. and {Mulkearns-Hubert}, Erin E. and Valentim, Claudia L. L. and Rich, Jeremy N.},
