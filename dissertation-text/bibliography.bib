@article{korthauerScDDStatisticalApproach2015,
  title = {{{scDD}}: {{A}} Statistical Approach for Identifying Differential Distributions in Single-Cell {{RNA-seq}} Experiments},
  author = {Korthauer, Keegan D. and Chu, Li-Fang and Newton, Michael A. and Li, Yuan and Thomson, James and Stewart, Ron and Kendziorski, Christina},
  year = {2015},
  journal = {bioRxiv},
  pages = {035501--035501},
  doi = {10.1101/035501},
  abstract = {The ability to quantify cellular heterogeneity is a major advantage of single-cell technologies. Although understanding such heterogeneity is of primary interest in a number of studies, for convenience, statistical methods often treat cellular heterogeneity as a nuisance factor. We present a novel method to characterize differences in expression in the presence of distinct expression states within and among biological conditions. Using simulated and case study data, we demonstrate that the modeling framework is able to detect differential expression patterns of interest under a wide range of settings. Compared to existing approaches, scDD has higher power to detect subtle differences in gene expression distributions that are more complex than a mean shift, and is able to characterize those differences. The freely available R package scDD implements the approach.},
  keywords = {cellular heterogeneity,differential expression,mixture modeling,single-cell rna-seq,{Single-cell RNA-seq,Differential expression,Cellul}},
  file = {/Users/ckmah/Zotero/storage/QKLTTB2T/Korthauer et al. - 2015 - scDD A statistical approach for identifying differential distributions in single-cell RNA-seq experiments.pdf}
}
