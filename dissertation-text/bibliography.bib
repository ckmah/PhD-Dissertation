@article{martyMHCIGenotypeRestricts2017,
  title = {{{MHC-I Genotype Restricts}} the {{Oncogenic Mutational Landscape}}},
  author = {Marty, Rachel and Kaabinejadian, Saghar and Rossell, David and Slifker, Michael J. and {van de Haar}, Joris and Engin, Hatice Billur and {de Prisco}, Nicola and Ideker, Trey and Hildebrand, William H. and {Font-Burgada}, Joan and Carter, Hannah},
