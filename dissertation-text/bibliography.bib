@article{yuRBMS1SuppressesColon2020,
  title = {{{RBMS1 Suppresses Colon Cancer Metastasis}} through {{Targeted Stabilization}} of {{Its mRNA Regulon}}},
  author = {Yu, Johnny and Navickas, Albertas and Asgharian, Hosseinali and Culbertson, Bruce and Fish, Lisa and Garcia, Kristle and Olegario, John Paolo and Dermit, Maria and Dodel, Martin and H{\"a}nisch, Benjamin and Luo, Yikai and Weinberg, Ethan M. and Dienstmann, Rodrigo and Warren, Robert S. and Mardakheh, Faraz K. and Goodarzi, Hani},
