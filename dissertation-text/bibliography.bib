@techreport{qiuSpateoMultidimensionalSpatiotemporal2022,
  type = {Preprint},
  title = {Spateo: Multidimensional Spatiotemporal Modeling of Single-Cell Spatial Transcriptomics},
  shorttitle = {Spateo},
  author = {Qiu, Xiaojie and Zhu, Daniel Y. and Yao, Jiajun and Jing, Zehua and Zuo, Lulu and Wang, Mingyue and Min, Kyung Hoi (Joseph) and Pan, Hailin and Wang, Shuai and Liao, Sha and Lai, Yiwei and Hao, Shijie and Lu, Yuancheng Ryan and Hill, Matthew and {Martin-Rufino}, Jorge D. and Weng, Chen and {Riera-Escandell}, Anna Maria and Chen, Mengnan and Wu, Liang and Zhang, Yong and Wei, Xiaoyu and Li, Mei and Huang, Xin and Xiang, Rong and Yang, Zhuoxuan and Liu, Chao and Xia, Tianyi and Liang, Yingxin and Xu, Junqiang and Hu, Qinan and Hu, Yuhui and Zhu, Hongmei and Li, Yuxiang and Chen, Ao and Esteban, Miguel A. and Gu, Ying and Lauffenburger, Douglas A. and Xu, Xun and Liu, Longqi and Weissman, Jonathan S. and Liu, Shiping and Bai, Yinqi},
  year = {2022},
  month = dec,
  institution = {{Systems Biology}},
  doi = {10.1101/2022.12.07.519417},
  urldate = {2023-08-27},
  abstract = {Cells do not live in a vacuum, but in a milieu defined by cell\textendash cell communication that can be measured via emerging high-resolution spatial transcriptomics approaches. However, analytical tools that fully leverage such data for kinetic modeling remain lacking. Here we present Spateo (             aristoteleo/spateo-release             ), a general framework for quantitative spatiotemporal modeling of single-cell resolution spatial transcriptomics. Spateo delivers novel methods for digitizing spatial layers/columns to identify spatially-polar genes, and develops a comprehensive framework of cell-cell interaction to reveal spatial effects of niche factors and cell type-specific ligand-receptor interactions. Furthermore, Spateo reconstructs 3D models of whole embryos, and performs 3D morphometric analyses. Lastly, Spateo introduces the concept of ``morphometric vector field'' of cell migrations, and integrates spatial differential geometry to unveil regulatory programs underlying various organogenesis patterns of Drosophila. Thus, Spateo enables the study of the ecology of organs at a molecular level in 3D space, beyond isolated single cells.},
  langid = {english},
