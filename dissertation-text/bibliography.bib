@article{wuSinglecellMorphologyEncodes2020,
  title = {Single-Cell Morphology Encodes Metastatic Potential},
  author = {Wu, Pei-Hsun and Gilkes, Daniele M. and Phillip, Jude M. and Narkar, Akshay and Cheng, Thomas Wen-Tao and Marchand, Jorge and Lee, Meng-Horng and Li, Rong and Wirtz, Denis},
  year = {2020},
