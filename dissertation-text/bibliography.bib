@article{fuUFoldFastAccurate2022,
  title = {{{UFold}}: Fast and Accurate {{RNA}} Secondary Structure Prediction with Deep Learning},
  shorttitle = {{{UFold}}},
  author = {Fu, Laiyi and Cao, Yingxin and Wu, Jie and Peng, Qinke and Nie, Qing and Xie, Xiaohui},
  year = {2022},
  month = feb,
  journal = {Nucleic Acids Research},
  volume = {50},
  number = {3},
  pages = {e14-e14},
  issn = {0305-1048, 1362-4962},
  doi = {10.1093/nar/gkab1074},
  urldate = {2023-08-27},
  abstract = {For many RNA molecules, the secondary structure is essential for the correct function of the RNA. Predicting RNA secondary structure from nucleotide sequences is a long-standing problem in genomics, but the prediction performance has reached a plateau over time. Traditional RNA secondary structure prediction algorithms are primarily based on thermodynamic models through free energy minimization, which imposes strong prior assumptions and is slow to run. Here, we propose a deep learningbased method, called UFold, for RNA secondary structure prediction, trained directly on annotated data and base-pairing rules. UFold proposes a novel image-like representation of RNA sequences, which can be efficiently processed by Fully Convolutional Networks (FCNs). We benchmark the performance of UFold on both within- and cross-family RNA datasets. It significantly outperforms previous methods on within-family datasets, while achieving a similar performance as the traditional methods when trained and tested on distinct RNA families. UFold is also able to predict pseudoknots accurately. Its prediction is fast with an inference time of about 160 ms per sequence up to 1500 bp in length. An online web server running UFold is available at https://ufold.ics.uci.edu. Code is available at https: //github.com/uci-cbcl/UFold.},
  langid = {english},
  file = {/Users/ckmah/Zotero/storage/IAR4HBGY/Fu et al. - 2022 - UFold fast and accurate RNA secondary structure p.pdf}
}
