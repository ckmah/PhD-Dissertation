  title = {Alternative {{Splicing}} as a {{Target}} for {{Cancer Treatment}}},
  author = {{Martinez-Montiel}, Nancy and {Rosas-Murrieta}, Nora Hilda and Anaya Ruiz, Maricruz and {Monjaraz-Guzman}, Eduardo and {Martinez-Contreras}, Rebeca},
