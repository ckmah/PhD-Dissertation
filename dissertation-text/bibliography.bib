@article{kimDecomposingOncogenicTranscriptional2017b,
  title = {Decomposing {{Oncogenic Transcriptional Signatures}} to {{Generate Maps}} of {{Divergent Cellular States}}},
  author = {Kim, Jong Wook and Abudayyeh, Omar O. and Yeerna, Huwate and Yeang, Chen-Hsiang and Stewart, Michelle and Jenkins, Russell W. and Kitajima, Shunsuke and Konieczkowski, David J. and {Medetgul-Ernar}, Kate and Cavazos, Taylor and Mah, Clarence and Ting, Stephanie and Van Allen, Eliezer M. and Cohen, Ofir and Mcdermott, John and Damato, Emily and Aguirre, Andrew J. and Liang, Jonathan and Liberzon, Arthur and Alexe, Gabriella and Doench, John and Ghandi, Mahmoud and Vazquez, Francisca and Weir, Barbara A. and Tsherniak, Aviad and Subramanian, Aravind and {Meneses-Cime}, Karina and Park, Jason and Clemons, Paul and Garraway, Levi A. and Thomas, David and Boehm, Jesse S. and Barbie, David A. and Hahn, William C. and Mesirov, Jill P. and Tamayo, Pablo},
