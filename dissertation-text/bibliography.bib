  langid = {english},
  pmid = {28978689},
  file = {/Users/ckmah/Zotero/storage/HHR64U36/Jurtz et al. - 2017 - NetMHCpan-4.0 Improved Peptide–MHC Class I Intera.pdf;/Users/ckmah/Zotero/storage/IM2J5RAY/3360.html}
}
