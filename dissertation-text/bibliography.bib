@article{mackChromatinLandscapesReveal2019,
  title = {Chromatin Landscapes Reveal Developmentally Encoded Transcriptional States That Define Human Glioblastoma},
  author = {Mack, Stephen C and Singh, Irtisha and Wang, Xiuxing and Hirsch, Rachel and Wu, Quilian and Villagomez, Rosie and Bernatchez, Jean A and Zhu, Zhe and Gimple, Ryan C and Kim, Leo J Y and {Others}},
