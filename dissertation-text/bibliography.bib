@article{wangEASIFISHThickTissue2021,
  title = {{{EASI-FISH}} for Thick Tissue Defines Lateral Hypothalamus Spatio-Molecular Organization},
  author = {Wang, Yuhan and Eddison, Mark and Fleishman, Greg and Weigert, Martin and Xu, Shengjin and Wang, Tim and Rokicki, Konrad and Goina, Cristian and Henry, Fredrick E. and Lemire, Andrew L. and Schmidt, Uwe and Yang, Hui and Svoboda, Karel and Myers, Eugene W. and Saalfeld, Stephan and Korff, Wyatt and Sternson, Scott M. and Tillberg, Paul W.},
