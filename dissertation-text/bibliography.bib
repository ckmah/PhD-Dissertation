@article{veeman2008,
  title = {{\emph{Chongmague}} Reveals an Essential Role for Laminin-Mediated Boundary Formation in Chordate Convergence and Extension Movements},
  author = {Veeman, Michael T. and Nakatani, Yuki and Hendrickson, Carolyn and Ericson, Vivian and Lin, Clarissa and Smith, William C.},
  year = {2008},
