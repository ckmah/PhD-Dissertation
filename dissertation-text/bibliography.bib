@article{nakamura2012,
  title = {Three-Dimensional Anatomy of the {{Ciona}} Intestinalis Tailbud Embryo at Single-Cell Resolution},
  author = {Nakamura, Mitsuru J. and Terai, Jun and Okubo, Reiko and Hotta, Kohji and Oka, Kotaro},
