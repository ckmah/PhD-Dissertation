@article{nguyenMappingRNARNA2016,
  title = {Mapping {{RNA}}\textendash{{RNA}} Interactome and {{RNA}} Structure in Vivo by {{MARIO}}},
  author = {Nguyen, Tri C. and Cao, Xiaoyi and Yu, Pengfei and Xiao, Shu and Lu, Jia and Biase, Fernando H. and Sridhar, Bharat and Huang, Norman and Zhang, Kang and Zhong, Sheng},
