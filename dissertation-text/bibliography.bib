@article{rubioDoxorubicinBindsDuplex2016,
  title = {Doxorubicin Binds to Duplex {{RNA}} with Higher Affinity than {{ctDNA}} and Favours the Isothermal Denaturation of Triplex {{RNA}}},
  author = {Rubio, Ana R. and Busto, Natalia and Leal, Jos{\'e} M. and Garc{\'i}a, Bego{\~n}a},
  year = {2016},
