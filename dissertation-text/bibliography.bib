@article{cieslaOncogenicTranslationDirects2021,
  title = {Oncogenic Translation Directs Spliceosome Dynamics Revealing an Integral Role for {{SF3A3}} in Breast Cancer},
  author = {Cie{\'s}la, Maciej and Ngoc, Phuong Cao Thi and Cordero, Eugenia and Martinez, {\'A}lvaro Sejas and Morsing, Mikkel and Muthukumar, Sowndarya and Beneventi, Giulia and Madej, Magdalena and Munita, Roberto and J{\"o}nsson, Terese and L{\"o}vgren, Kristina and Ebbesson, Anna and Nodin, Bj{\"o}rn and Hedenfalk, Ingrid and Jirstr{\"o}m, Karin and {Vallon-Christersson}, Johan and Honeth, Gabriella and Staaf, Johan and Incarnato, Danny and Pietras, Kristian and Bosch, Ana and Bellodi, Cristian},
