  author = {Tracy, Tara E. and {Madero-P{\'e}rez}, Jesus and Swaney, Danielle L. and Chang, Timothy S. and Moritz, Michelle and Konrad, Csaba and Ward, Michael E. and Stevenson, Erica and H{\"u}ttenhain, Ruth and Kauwe, Grant and Mercedes, Maria and {Sweetland-Martin}, Lauren and Chen, Xu and Mok, Sue-Ann and Wong, Man Ying and Telpoukhovskaia, Maria and Min, Sang-Won and Wang, Chao and Sohn, Peter Dongmin and Martin, Jordie and Zhou, Yungui and Luo, Wenjie and Trojanowski, John Q. and Lee, Virginia M.Y. and Gong, Shiaoching and Manfredi, Giovanni and Coppola, Giovanni and Krogan, Nevan J. and Geschwind, Daniel H. and Gan, Li},
