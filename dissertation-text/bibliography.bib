@article{vandenbonClusteringindependentMethodFinding2020,
  title = {A Clustering-Independent Method for Finding Differentially Expressed Genes in Single-Cell Transcriptome Data},
  author = {Vandenbon, Alexis and Diez, Diego},
  year = {2020},
  month = aug,
  journal = {Nature Communications},
  volume = {11},
  number = {1},
  pages = {4318},
  issn = {2041-1723},
  doi = {10.1038/s41467-020-17900-3},
  urldate = {2023-08-27},
  abstract = {Abstract             A common analysis of single-cell sequencing data includes clustering of cells and identifying differentially expressed genes (DEGs). How cell clusters are defined has important consequences for downstream analyses and the interpretation of results, but is often not straightforward. To address this difficulty, we present singleCellHaystack, a method that enables the prediction of DEGs without relying on explicit clustering of cells. Our method uses Kullback\textendash Leibler divergence to find genes that are expressed in subsets of cells that are non-randomly positioned in a multidimensional space. Comparisons with existing DEG prediction approaches on artificial datasets show that singleCellHaystack has higher accuracy. We illustrate the usage of singleCellHaystack through applications on 136 real transcriptome datasets and a spatial transcriptomics dataset. We demonstrate that our method is a fast and accurate approach for DEG prediction in single-cell data. singleCellHaystack is implemented as an R package and is available from CRAN and GitHub.},
  langid = {english},
  file = {/Users/ckmah/Zotero/storage/5LZ33GQI/Vandenbon and Diez - 2020 - A clustering-independent method for finding differ.pdf}
}
